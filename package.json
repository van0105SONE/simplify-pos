{
  "name": "aung-pos-system",
  "version": "0.1.0",
  "private": true,
  "main": "electron/electron.js",
  "scripts": {
    "prisma": "prisma",
    "electron": "electron electron/electron.js",
    "desktop-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run electron\"",
    "seed": "ts-node prisma/seed.ts",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "desktop-build": "next build",
    "desktop-dist": "npm run desktop-build && npx electron-builder"
  },
  "build": {
    "appId": "com.aungpos.desktop",
    "productName": "AungPos",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "**/*",
      ".next/**/*",
      "electron/**/*",
      "package.json",
      "prisma/**/*",
      "out/**/*",
      "!node_modules/.cache",
      "!**/*.map"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.0",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/electron": "^1.4.38",
    "@types/printer": "^0.4.5",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-serve": "^2.1.1",
    "electron-store": "^10.1.0",
    "escpos": "^3.0.0-alpha.6",
    "escpos-usb": "^3.0.0-alpha.4",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.532.0",
    "next": "^15.4.5",
    "next-themes": "^0.4.6",
    "npm": "^11.5.2",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "react-router-dom": "^7.7.1",
    "recharts": "^2.15.4",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "wait-on": "^8.0.4",
    "zod": "^4.0.10",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19.1.9",
    "@types/react-day-picker": "^5.2.1",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "electron": "^37.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "postcss": "^8.5.6",
    "prisma": "^6.14.0",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3"
  }
}
